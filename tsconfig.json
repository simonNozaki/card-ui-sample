{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES5",
    "lib": ["ES2020", "DOM"],
    "esModuleInterop": true,
    "moduleResolution": "node",
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.vue",
    "./type/**/*.d.ts"
  ],
  "typeRoots": [
    "type",
    "node_modules/@types"
  ]
}